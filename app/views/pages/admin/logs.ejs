
<!doctype html>
<html lang="en" class="h-100">
    <head>
        <%- include('../../partials/head'); %>

        <!-- css -->
        <% if(typeof sheets!=='undefined') { %>
            <% for(let sheet of sheets) { %>
                <link href="/css/<%= sheet %>.css" rel="stylesheet">
            <% } %>
        <% } %>
    </head>

    <body class="h-100">
        <%- include('../../partials/header'); %>

        <div class="container-fluid pb-3 h-100" style="padding-top: 90px;">
            <div class="row w-100 h-100" style="flex-wrap:nowrap">
                <%- include('../../partials/sidebar', {level:''}); %>

                <main id="main-content" class="col overflow-auto">
                    <div class="row">
                        <div class="col">
                            <h5>
                                <i class="icon-database"></i>
                                Logs
                            </h5>
                        </div>
                    </div>

                    <div class="row mt-3">
                        <div class="col-xs-12 col-md-6">
                            <div class="input-group input-group-sm">
                                <input id="search" type="text" class="form-control">
                                <span class="input-group-text">Buscar</span>
                            </div>
                        </div>
                    </div>

                    <div class="row my-3">
                        <div class="col">
                            <div class="card border-light bg-primary shadow">
                                <div class="card-body">
                                    <table id="logs-table" class="display table bg-primary table-sm text-white" style="width: 100%;">
                                        <thead>
                                            <tr>
                                                <th>Id</th>
                                                <th>Ip Client</th>
                                                <th>Path</th>
                                                <th>Start</th>
                                                <th>Status</th>
                                                <th></th>
                                            </tr>
                                        </thead>
                                    </table>
                                </div>
                            </div>
                        </div>
                    </div>
                </main>
            </div>
        </div>

        <!-- Modal -->
        <%- include('../../partials/modal') %>
        
        <% if(typeof libs!=='undefined') { %>
            <% for(let lib of libs) { %>
                <script src='/js/<%= lib %>.js' type='text/javascript'></script>
            <% } %>
        <% } %>
    </body>
</html>